{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug millennium",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dist/Linux-Debug/millennium",
            "cwd": "${workspaceFolder}/dist/Linux-Debug/",
            "environment": [],
