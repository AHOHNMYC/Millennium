{
    "Hooks": [
        {
            "Interpolate": "webkit.css",
            "TargetCss": "https://community.akamai.steamstatic.com/public/shared/css/buttons.css*"
        },
        {
            "Interpolate": "webkit.css",
            "TargetCss": "https://store.akamai.steamstatic.com/public/shared/css/buttons.css*"
        },
        {
            "Interpolate": "libraryroot.custom.js",
            "TargetJs": "https://store.akamai.steamstatic.com/public/shared/javascript/shared_responsive_adapter.js*"
        }
    ],
    "Patches": [
        {
            "MatchRegexString": "SteamBrowser_Find",
            "TargetCss": "libraryroot.custom.css"
        },
        {
            "MatchRegexString": "^Steam$",
            "TargetCss": "libraryroot.custom.css"
        },
        {
            "MatchRegexString": "Shutdown",
            "TargetCss": "libraryroot.custom.css"
        },
        {
            "MatchRegexString": "friendsui-container",
            "TargetCss": "friends.custom.css"
        },
        {
            "MatchRegexString": "ModalDialogPopup",
            "TargetCss": "libraryroot.custom.css"
        },
        {
            "MatchRegexString": ".*Supernav$",
            "TargetCss": "libraryroot.custom.css"
        },
        {
            "MatchRegexString": ".*Menu$",
            "TargetCss": "libraryroot.custom.css"
        },
        {
            "MatchRegexString": ".*Properties.*",
            "TargetCss": "libraryroot.custom.css"
        },
        {
            "MatchRegexString": "Special Offers",
            "TargetCss": "libraryroot.custom.css"
        },
        {
            "MatchRegexString": "Steam Big Picture Mode",
            "TargetCss": "libraryroot.custom.css"
        }
    ],
    "UseDefaultPatches": false,
    "author": "Unknown",
    "description": "A somewhat complicated design for the Steam application, adding more color, contrast, and diversity to your Steam experience",
    "name": "Minimal Dark for Steam",
    "source": "https://api.github.com/repos/ShadowMonster99/millennium-steam-patcher/contents/root/minimal_dark_for_steam",
    "patch-notes": "https://api.github.com/repos/ShadowMonster99/millennium-steam-patcher/releases/latest",
    "version": "1.0.1"
}
